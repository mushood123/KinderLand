{
  "name": "KinderLandApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "ios-15": "react-native run-ios --simulator='iPhone 15'",
    "ios-air": "react-native run-ios --simulator='iPad Air (5th generation) (iOS 16.4)'",
    "ios-ipad": "react-native run-ios --simulator='iPad (10th generation)'",
    "ios-mini": "react-native run-ios --simulator='iPad mini (6th generation)'",
    "clean-cache": "rm -rf ~/Library/Developer/Xcode/DerivedData",
    "clean-files": "rm -rf yarn.lock && rm -rf Gemfile.lock && rm -rf node_modules && rm -rf ios/Pods && rm -rf ios/Gemfile.lock  && rm -rf ios/Podfile.lock && rm -rf ios/build && rm -rf ios/.xcode.env.local && rm -rf android/Gemfile.lock",
    "setup": "yarn; yarn gem:install; yarn pod:install; yarn gradle;",
    "gem:install": "bundle install; cd ios; bundle install; cd ..; cd android; bundle install; cd ..;",
    "pod:install": "cd ios; pod install; cd ..;",
    "gradle": "cd android; ./gradlew clean; cd ..",
    "lint": "eslint .",
    "start": "react-native start --reset-cache",
    "test": "jest"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native-masked-view/masked-view": "^0.3.2",
    "@react-native/new-app-screen": "0.80.1",
    "@react-navigation/bottom-tabs": "^7.4.2",
    "@react-navigation/native": "^7.1.14",
    "@react-navigation/stack": "^7.4.2",
    "axios": "^1.11.0",
    "react": "19.1.0",
    "react-native": "0.80.1",
    "react-native-date-picker": "^5.0.13",
    "react-native-gesture-handler": "^2.27.1",
    "react-native-reanimated": "^3.18.0",
    "react-native-safe-area-context": "^5.5.2",
    "react-native-screens": "^4.13.0",
    "react-native-signature-canvas": "^5.0.1",
    "react-native-svg": "^15.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "19.0.0",
    "@react-native-community/cli-platform-android": "19.0.0",
    "@react-native-community/cli-platform-ios": "19.0.0",
    "@react-native/babel-preset": "0.80.1",
    "@react-native/eslint-config": "0.80.1",
    "@react-native/metro-config": "0.80.1",
    "@react-native/typescript-config": "0.80.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "5.0.4"
  },
  "engines": {
    "node": ">=18"
  }
}
